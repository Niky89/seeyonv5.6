<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seeyon.v3x.hr.domain.StaffInfo" table="hr_staff_info">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="org_member_id" type="long" column="org_member_id"
			length="20" not-null="true" />
		<property name="sex" type="string" column="sex" length="10" />
		<property name="name" type="string" column="name" length="255" />
		<property name="usedname" type="string" column="usedname"
			length="255" />
		<property name="birthday" type="timestamp" column="birthday"
			length="19" />
		<property name="nation" type="string" column="nation" length="255" />
		<property name="birthplace" type="string" column="birthplace"
			length="255" />
		<property name="ID_card" type="string" column="ID_card"
			length="255" />

		<property name="edu_level" type="integer" column="edu_level"
			length="4" />
		<property name="marriage" type="integer" column="marriage"
			length="1" />
		<property name="political_position" type="integer" column="political_position"
			length="1" />
		<property name="work_starting_date" type="timestamp" column="work_starting_date"
			length="19" />
		<property name="working_time" type="integer" column="working_time"
			length="4" />
		<property name="specialty" type="string" column="specialty"
			length="255" />
		<property name="hobby" type="string" column="hobby" />
		<property name="record_wage" type="double" column="record_wage" />
		<property name="remark" type="string" column="remark" />
		<property name="email" type="string" column="email" length="255" />
		<property name="tel_number" type="string" column="tel_number"
			length="255" />
		<property name="qq" type="string" column="qq" length="255" />
		<property name="msn" type="string" column="msn" length="255" />
		<property name="image_id" type="long" column="image_id"
			length="20" />
		<property name="image_datetime" type="timestamp" column="image_datetime"
			length="19" />
		<property name="image_name" type="string" column="image_name"
			length="255" />
		<property name="self_image_name" type="string" column="self_image_name"
			length="255" />
		<property name="degreeLevel" type="string" column="degree_level"
			length="255" />
		<property name="domicile_place" type="string" column="domicile_place"
			length="255" />
		<property name="health" type="string" column="health" length="255" />
		<property name="infectious_diseases" type="string"
			column="infectious_diseases" length="255" />
		<property name="frist_work_date" type="timestamp" column="frist_work_date"
			length="19" />
		<property name="isok" type="string" column="isok" length="100" />
		<property name="work_language" type="string" column="work_language"
			length="255" />
			<property name="lawNo" type="string" column="lawNo"
			length="255" />
			<property name="pralawNo" type="string" column="pralawNo"
			length="255" />
			<property name="judicial_qualificationNo" type="string" column="judicial_qualificationNo"
			length="255" />
	</class>
</hibernate-mapping>
