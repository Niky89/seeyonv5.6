<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seeyon.v3x.hr.domain.HrSalaryPassword" table="hr_salary_password">
		<id name="id" type="java.lang.Long">
			<column name="id" />
		</id>
		
		<property 
		name="userId"
		type="java.lang.Long"
		column="user_id" 
		length="20"
		/>	
		 
		<property
          name="createDate"
          type="timestamp"
          column="create_date"
          length="19"
        />
        <property
          name="updateDate"
          type="timestamp"
          column="update_date"
          length="19"
        />
       <property
          name="salaryPassword"
          type="string"
          column="salary_password"
          length="255"
       />
	</class>
</hibernate-mapping>
